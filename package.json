{
  "name": "neco",
  "version": "0.1.1",
  "private": false,
  "files": [
    "es",
    "lib"
  ],
  "module": "es/index.js",
  "main": "lib/index.js",
  "typings": "es/index.d.ts",
  "description": "Learn Storybook design system",
  "repository": "git@github.com:love962965494/learnstorybook-design-system.git",
  "author": "shenchao <shenchao20@jd.com>",
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.10",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@storybook/cli": "^5.3.14",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "classnames": "^2.2.6",
    "immer": "^6.0.3",
    "less": "^3.11.1",
    "mobx": "^5.15.4",
    "polished": "^3.4.4",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "antd": ">=3.20.1",
    "react-router": ">=5.1.2",
    "react-router-dom": ">=5.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "test": "react-scripts test --passWithNoTests",
    "prepare": "install-peers",
    "eject": "react-scripts eject",
    "compile": "gulp compile",
    "debug-compile": "node --inspect-brk ./node_modules/gulp/bin/gulp.js compile",
    "storybook": "start-storybook -p 9009 -s public",
    "debug-storybook": "node --inspect-brk ./node_modules/@storybook/react/bin/index.js -p 9009 -s public ",
    "build-storybook": "build-storybook -s public",
    "build-storybook-docs": "build-storybook -s public --docs",
    "release": "auto shipit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": [
      "eslint --max-warnings=-1",
      "npx prettier --write"
    ],
    "src/**/*.less": [
      "stylelint --fix",
      "npx prettier --write"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "@typescript-eslint",
      "react"
    ],
    "rules": {
      "@typescript-eslint/member-delimiter-style": [
        "error",
        {
          "multiline": {
            "delimiter": "none",
            "requireLast": true
          },
          "singleline": {
            "delimiter": "semi",
            "requireLast": false
          }
        }
      ],
      "@typescript-eslint/explicit-function-return-type": "off"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 120,
    "endOfLine": "auto",
    "arrowParens": "avoid"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@storybook/addon-a11y": "^5.3.14",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-storysource": "^5.3.14",
    "@storybook/addon-viewport": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.14",
    "@types/classnames": "^2.2.10",
    "@types/mockjs": "^1.0.2",
    "@types/prop-types": "^15.7.3",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.4",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "antd": "^4.1.4",
    "auto": "^9.15.8",
    "autoprefixer": "^9.7.5",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-mdx": "^1.6.8",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.3",
    "install-peers-cli": "^2.2.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.9",
    "merge2": "^1.3.0",
    "minimist": "^1.2.5",
    "mockjs": "^1.1.0",
    "postcss": "^7.0.27",
    "prettier": "^2.0.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "through2": "^3.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
  "license": "MIT"
}